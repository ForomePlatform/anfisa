{
    "file-path-def": {"WORK": "${HOME}/../a-setup"},
    "host": "0.0.0.0",
    "port": 8190,
    "html-base": "/anfisa/",
    "html-title": "Anfisa",
    "mongo-db": "Anfisa",
    "-workspaces": [
        {
            "name": "Personal_Genome_3140",
            "mongo-name": "bgm9001",
            "file": "${WORK}/data/PGP3140.json"
        }
    ],
    "workspaces": [],
    "dir-files": [
        ["/ui/images", "${HOME}/int_ui/images"],
        ["/ui", "${HOME}/int_ui/files"],
        ["--/ui", "${WORK}/ui"],
        ["/excel", "${WORK}/export/work"]], 
    "--mirror-ui": ["${HOME}/int_ui/files", "${WORK}/ui"],
    "export": {
        "excel-template": "${WORK}/export/SEQaBOO_output_template_0730.xlsx",
        "work-dir": "${WORK}/export/work"
    },
    "link-base": ["anfisa.forome.org", "Local"],
    "logging": { 
        "version": 1,
        "disable_existing_loggers": false,
        "formatters": { 
                "standard": { 
                "format": "%(asctime)s [%(levelname)s] %(name)s: %(message)s"
            }
        },
        "handlers": { 
            "default": { 
                "level": "INFO",
                "formatter": "standard",
                "class": "logging.FileHandler",
                "filename": "${WORK}/logs/anfisa.log"
            }
        },
        "loggers": { 
            "": { 
                "handlers": ["default"],
                "level": "INFO",
                "propagate": true
            }
        } 
    },
    
    "xl-sets": [
        {"title": "XL experimental", "name": "exp", "file": "${WORK}/xl-set.js"}],
    "druid": {
        "url_index": "http://localhost:8090/druid/indexer/v1/task",
        "url_query": "http://localhost:8082/druid/v2",
        "url_sql":   "http://localhost:8082/druid/v2/sql",
        "url_coord": "http://localhost:8081/druid/coordinator/v1"
    }
}