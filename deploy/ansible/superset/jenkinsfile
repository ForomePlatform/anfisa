pipeline {
  agent { node { label 'master' } }
  stages {

    stage('Checkout git') {
      parallel {
        stage('Checkout code') {
          steps {
            git branch: 'master', credentialsId: 'ubuntu', url: 'git@github.com:Kvitneset/RSM-Analytics.git'
          }
        }
      }
    }
    stage('Deploy code') {
      steps {
        dir('ansible') {
          sh '''#!/bin/bash
                ansible-playbook playbook.yaml -i inventory.yaml  --tags superset -K --key-file=/var/jenkins_home/jenkins_private
            '''
        }
      }
    }
  }
}
